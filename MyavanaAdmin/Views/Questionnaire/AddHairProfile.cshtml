                        }
                        if (response.healthSummary == null || response.healthSummary == '' || response.hairId == null) {
                          
                            $('#txtHealthSummary ').val($('#dvHealthSummary').text());
                        }
                        else {
                            $('#txtHealthSummary ').val(response.healthSummary);
                        }

                    }
                    if (tabno == "Tab2") {
                        loadTab2(response);
                    }
                    //-----------------------------3rd tab--------------------------------------------------------------
                    if (tabno == "Tab3") {
                        
                        var pType = @Html.Raw(Json.Serialize(content.Result.ProductTypes));
                        loadTab3(pType, response);
                    }
                        //--------------------------4th tab----------------------------
                    if (tabno == "Tab4")
                    {
                        if (response != null) {
                            loadTab4(response);
                        }
                    }

                    }//response!=null closing
                $('.preloader').css('display', 'none');
            },//success closing

            error: function (response) {

            },
            complete: function () {

            }
        });//ajax call closing
       }
</script>


